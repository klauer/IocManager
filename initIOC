#!/bin/sh

# Find our standard configuration.
host=`hostname -s`
cfg=`awk /$host/'{print $2;}'  /reg/g/pcds/pyps/config/hosts.special`
if [ "${cfg}X" == "X" ]; then
    cfg=`echo $host | awk '{print substr($0,5,3);}' -`
fi
echo Using configuration $cfg.

export SCRIPTROOT=/reg/g/pcds/pyps/config/$cfg/iocmanager

source $SCRIPTROOT/initIOC.hutch
